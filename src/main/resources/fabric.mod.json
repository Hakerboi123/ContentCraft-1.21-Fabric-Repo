{
	"schemaVersion": 1,
	"id": "contentcraft",
	"version": "${version}",
	"name": "Content Craft",
	"description": "Do you feel like minecraft has become boring? Well Contentcraft is here to fix that! meet the all in one mod fixing minecraft's progression, advancements, items and way more!",
	"authors": [
		"Hakerboi",
		"JuiceBoxStudios"
	],
	"contact": {
		"homepage": "https://github.com/Hakerboi123/ContentCraft-1.21-Fabric-Repo",
		"sources": "https://github.com/Hakerboi123/ContentCraft-1.21-Fabric-Repo"
	},
	"license": "Apache License 2.0",
	"icon": "assets/contentcraft/ContentCraft Logo.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"ie.juiceboxstudios.contentcraft.ContentCraft"
		],
		"fabric-datagen": [
			"ie.juiceboxstudios.contentcraft.ContentCraftDataGenerator"
		],
		"client": [
			"ie.juiceboxstudios.contentcraft.ContentCraftClient"
		]
	},
	"mixins": [
		"contentcraft.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}